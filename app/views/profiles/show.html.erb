
   <div class="content">

    <div style="text-align: left; margin-top:100px">
      <h1>User Profile</h1>
    </div>

    <div class="profile-details">
      <p><strong>First Name:</strong> <%= @user.f_name %></p>
      <p><strong>Last Name:</strong> <%= @user.l_name %></p>
      <p><strong>Email:</strong> <%= @user.email %></p>
      <p><strong>Date of Birth:</strong> <%= @user.DateofBirth %></p>
      <p><strong>Account Number:</strong> <%= @user.accountnumber %></p>
      <p><strong>IFSC:</strong> <%= @user.ifsc %></p>
      <p><strong>Mobile Number:</strong> <%= @user.MobileNumber %></p>
      <p><strong>Joining Date:</strong> <%= @user.JoiningDate %></p>
      <p><strong>Is Active:</strong> <%= @user.isactive %></p>
    </div>
  </div>
  <div style="position: absolute; top: 130px; right: 10px;">
  <%= link_to 'Edit Profile', edit_user_registration_path, style: "color: white; text-decoration: none; padding: 10px; background-color: #365486; border-radius: 5px;" %>
</div>
 <%= render 'layouts/sidebar' %>
